@model HomeViewModel

<section class="esh-catalog-hero">
    <div class="container">
        <img class="esh-catalog-title" src="~/Images/main_banner_message.png">
    </div>
</section>

<div class="text-md-center">
    <h2>All cars on sale</h2>
</div>

<div class="container">
    <div class="row">
        <partial name="_Carousel" />
    </div>
    <div class="row">
        <h2>Car on sale</h2>
    </div>
    <div class="row">
        @foreach (var car in Model.CarOnSale)
        {
            <partial name="_carCard" model="car" />
        }
    </div>
</div>